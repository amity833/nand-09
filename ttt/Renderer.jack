class Renderer {
    field Board board;
    constructor Renderer new(Board _board){
        let board = _board;
        return this;
    }

    method void draw(){
        do DrawHelper.drawO(100,100,100,5,true);
        do DrawHelper.drawO(50,100,30,12,true);
        do DrawHelper.drawO(150,100,30,12,true);
        do DrawHelper.drawX(100,100,100,5,true);
        return;
    }
}